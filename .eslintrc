{
  "env": {
    "commonjs": true,
    "es6": true,
    "browser": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "no-trailing-spaces": "warn",
    "max-len": [
      "warn",
      108
    ],
    "no-console": "off",
    "no-unused-vars": "warn",
    "indent": [
      "warn",
      2
    ],
    "quotes": [
      "warn",
      "double"
    ],
    "semi": [
      "warn",
      "always"
    ]
  }
}